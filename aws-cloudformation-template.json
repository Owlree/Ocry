{
    "Resources": {
        "DocperDB": {
            "Type": "AWS::DynamoDB::Table",
            "Properties": {
                "TableName": {
                    "Ref": "DocperDBTableName"
                },
                "KeySchema": [
                    {
                        "AttributeName": "Name",
                        "KeyType": "HASH"
                    }
                ],
                "ProvisionedThroughput": {
                    "ReadCapacityUnits": "5",
                    "WriteCapacityUnits": "5"
                },
                "AttributeDefinitions": [
                    {
                        "AttributeName": "Name",
                        "AttributeType": "S"
                    },
                    {
                        "AttributeName": "TheirURL",
                        "AttributeType": "S"
                    },
                    {
                        "AttributeName": "OurURL",
                        "AttributeType": "S"
                    },
                    {
                        "AttributeName": "Checksum",
                        "AttributeType": "S"
                    }
                ]
            }
        }
    }
}
